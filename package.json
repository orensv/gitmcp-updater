{
  "name": "gitmcp-updater",
  "displayName": "GitMCP Updater",
  "description": "Quickly add a GitMCP server to your mcp.json from a GitHub URL.",
  "version": "0.0.1",
  "publisher": "orensv-gitmcp-updater",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orensv/gitmcp-updater.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gitmcp.updateFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitmcp.updateFile",
        "title": "GitMCP: Update mcp.json from GitHub URL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.45.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
